#!/bin/sh
set -e
# Automatically added by dh_installudev
if [ "$1" = install ] || [ "$1" = upgrade ]; then
	if [ -e "/etc/udev/rules.d/85_lirc.rules" ]; then
		if [ "`md5sum \"/etc/udev/rules.d/85_lirc.rules\" | sed -e \"s/ .*//\"`" = \
		     "`dpkg-query -W -f='${Conffiles}' lirc | sed -n -e \"\\\\' /etc/udev/rules.d/85_lirc.rules '{s/ obsolete$//;s/.* //p}\"`" ]
		then
			rm -f "/etc/udev/rules.d/85_lirc.rules"
		fi
	fi
fi
